	"scripts": {
		"reset": "rm ./node_modules/react-scripts/config/webpack.config.js",
		"mover": "cp ./webpack.config.js ./node_modules/react-scripts/config/webpack.config.js",
		"start": "react-scripts start",
		"dev": "nodemon index.js",
		"lint": "eslint src --ext .js",
		"lint:fix": "npm run lint -- --fix",
		"build": "npm run reset && npm run mover && react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"eslint": "^9.21.0",
		"eslint-plugin-react": "^7.37.4"
	}
